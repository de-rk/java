		//bubble sort
		Scanner scanner=new Scanner(System.in);
		int arr[]=new int[10];
		for (int i=0;i<10;i++) {
			System.out.println("输入第"+i+"个数:");
			arr[i]=scanner.nextInt();
		}
		System.out.println(Arrays.toString(arr));
		int n=10; int t;
		for(int i = 1; i < n; i++)
				   {
				      for(int j = 0; j < n-i; j++)
				       {
				           if(arr[ j ] > arr[ j +1 ]) 
				            {
				                t = arr[ j ]; 
					            arr[ j ] = arr[ j +1 ]; 
					            arr[ j +1 ] = t;  
				            }
				       }
				  }
			System.out.println(Arrays.toString(arr));
			
		
	}
}
