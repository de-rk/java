package com.github.test;

public class Vehicle {
	private String brand;
	private String color;
	private double speed=0;
	public Vehicle(String a,String b,double c) {
		brand=a;
		color=b;
		speed=0;
	}
	public void run(double c) {
		speed=c;
		System.out.println(color+"色的"+brand+"汽车以"+speed+"速度在行驶");
	}
}
