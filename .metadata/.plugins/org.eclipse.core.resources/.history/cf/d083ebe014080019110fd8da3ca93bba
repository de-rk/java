package com.github.test;


public class Student {
	private int age;
	
	public int getAge() {
		return age;
	}
	public int setAge(int age) {
		if (age>35||age<18) {
			return this.age;
		}
		else {
			this.age=age;
			return age;
			}
		}
	
	int d=0;
	public int sort(double[] sum) {
		for (int i=0;i<sum.length;i++)
			if (sum[i]>sum[i+1]) {
				sum[i+1]=sum[i];
				d=i;
			}
		return sum[d];
		}
}
