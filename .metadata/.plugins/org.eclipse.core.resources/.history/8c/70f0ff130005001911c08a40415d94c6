		//System.out.println(getY(scanner.next().split(",")));
		//接收字符串，根据','分割成String型数组，再传给getY，输出返还值
		FuncTest getZ=new FuncTest();
		//实例化FuncTest类
		//getZ.getZ();
		//调用getZ方法
		//System.out.println(getZ.year(scanner.nextInt()));
		//大于两百的最小质数
		//getZ.zhishu();
		//排序
		//getZ.maopao(scanner.next());
		//重载方法，实现不同类型参数运算
		//shu('4','*','7');
		//计算不同图形的面积
		getZ.mianji(5,5,5,6);
