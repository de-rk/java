package com.github.test;

public class Laptop {
	int cpu;
	String color;
	public Laptop() {}//无参构造方法
	public Laptop(String color,int cpuu) {
		this.color=color;
		this.cpu=cpu;
	}
	public void print(String color,int cpu) {
		System.out.println("颜色"+color+"cpu"+cpu);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		new Laptop().print("red", 5332);
	}

}
